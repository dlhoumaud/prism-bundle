{
    "name": "prism/bundle",
    "description": "Symfony bundle for multi-scope data isolation, complete traceability and intelligent purging",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "symfony",
        "bundle",
        "fixtures",
        "prism",
        "scope",
        "isolation",
        "testing",
        "data-seeding",
        "hexagonal-architecture",
        "multi-scope"
    ],
    "authors": [
        {
            "name": "David Lhoumaud",
            "email": "dlhoumaud@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^5.4|^6.0|^7.0",
        "symfony/console": "^5.4|^6.0|^7.0",
        "symfony/yaml": "^5.4|^6.0|^7.0",
        "symfony/config": "^5.4|^6.0|^7.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0",
        "doctrine/dbal": "^3.0|^4.0",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Prism\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Prism\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "require": "^6.0|^7.0"
        }
    }
}
